#app {
    layout: grid;
    grid-size: 2 3;
    grid-rows: 1 3 1fr;
    grid-columns: 20% 1fr;
    grid-gutter: 1;
    margin: 1 2;
}

#patheditor {
    layout: grid;
    grid-size: 3 1;
    height: 3;
    grid-columns: 7 20 1fr;
    column-span: 2
}

#patheditorlabel {
    width: 7;
    height: 3;
    content-align: center middle;
}

#patheditorinput {
}

#explorer {
    width: 100%
}

#bookmarks {
}

#header {
    column-span: 2;
    content-align: left middle;
    height: 2;
}